<% provide(:title, "Contact") %>
<div class="banner-img-div-940">
    <div class="contact-translucent">
        <div class="div-90 bgt">
            <div class="h10-w100 bgt">
                <h1 class="bgt text-center">We would like to hear from you.</h1>
            </div>
            <div class="div90cm bgt pt-10">
                <form class="bgt" method="post" action="#">

                    <div class="bgt div-90cp">
                        <label class="">
                            <span class="label">Name</span>
                            <input class="input-box mt-10" type="text" name="name">
                        </label>
                    </div>

                    <div class="bgt div-90cp pt-20">
                        <label class="">
                            <span class="label">Email</span>
                            <input class="input-box mt-10" type="text" name="email">
                        </label>
                    </div>

                    <div class="bgt div-90cp pt-20">
                        <label class="">
                            <span class="label">Company</span>
                            <input class="input-box mt-10" type="text" name="company">
                        </label>
                    </div>

                    <div class="bgt div-90cp pt-20">
                        <label class="">
                            <span class="label">Telephone</span>
                            <input class="input-box mt-10" type="text" name="telephone">
                        </label>
                    </div>
                    <div class="bgt div-90cp pt-20">
                        <label class="">
                            <span class="label">Message</span>
                        </label>
                    </div>
                    <div class="bgt div-90cp pt-10">
                        <textarea></textarea>
                    </div>
                    <div class="div-90cp bgt pt-30">
                    <button class="ak-button login-button" type="button btn">SEND</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
